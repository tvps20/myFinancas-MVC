<script type="text/javascript">
    function mostrarModalDetalhes(id, path) {
        path = path + "/" + id;
            $(document).ready(function () {
                $.ajax({
                type: "GET",
                    traditional: true,
                    url: ''.concat(path),
                success: function (data) {
                    document.getElementById('modalDetalhes').innerHTML = data;
                    $('#detalhesModalLancamento').modal('show');
                },
                error: function () {
                    alert("Erro na tentativa de emissão!");
                }
            });
        })
    }
</script>
